conda create -n hifiasm_mod

conda activate hifiasm_mod

conda install _libgcc_mutex=0.1=conda_forge
#conda-forge/linux-64::_libgcc_mutex-0.1-conda_forge

conda install _openmp_mutex=4.5=2_gnu
#conda-forge/linux-64::_openmp_mutex-4.5-2_gnu
#conda-forge/linux-64::libgomp-14.2.0-h77fa898_1

conda install libgcc=14.2.0=h77fa898_1
#conda-forge/linux-64::libgcc-14.2.0-h77fa898_1

conda install libgomp=14.2.0=h77fa898_1
#

conda install libstdcxx=14.2.0=hc0a3c3a_1
#conda-forge/linux-64::libstdcxx-14.2.0-hc0a3c3a_1

conda install libzlib=1.3.1=hb9d3cd8_2
#conda-forge/linux-64::libzlib-1.3.1-hb9d3cd8_2